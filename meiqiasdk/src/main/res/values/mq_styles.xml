<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MQViewPhotoDialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>

    <style name="MQBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/mq_colorPrimary</item>
        <item name="android:windowBackground">@color/mq_windowBackground</item>
        <item name="colorAccent">@color/mq_colorAccent</item>
    </style>

    <style name="MQTheme" parent="MQBaseTheme" />

    <style name="MQWrapMatch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MQMatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MQWrapWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MQMatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MQMatchAuto">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="MQAutoMatch">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MQWrapAuto">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="MQAutoWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MQWrapMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQWrapMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MQMatchWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQMatchWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MQWrapWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQWrapWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MQMatchMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQMatchMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MQMatchAuto.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQMatchAuto.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MQAutoMatch.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQAutoMatch.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MQWrapAuto.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQWrapAuto.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MQAutoWrap.Vertical">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MQAutoWrap.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>
</resources>